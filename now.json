{
    "version": 2,
	"name": "trevor-atlas",
	"alias": [
		"trevoratlas.com",
		"www.trevoratlas.com",
		"blog.trevoratlas.com"
	],
    "builds": [
        {
            "src": "package.json",
            "use": "@now/static-build",
            "config": {
                "distDir": "public"
            }
        },
		{ "src": "content/", "use": "@now/static" },
	],
	"routes": [
		{
			"src": "/(.*)",
			"status": 301,
			"headers": {
				"Location": "https://trevoratlas.com/$1"
			}
		}
	 ]
}
